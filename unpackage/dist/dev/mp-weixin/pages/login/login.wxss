
.dailogView{
	width: 600rpx;
	padding: 20rpx;
	height: auto;
	-webkit-border-radius: 20rpx;
	        border-radius: 20rpx;
	background-color: #ffffff;
}
.dailogView .title{
	border-bottom: 1rpx solid #eeeeee;
	height: 70rpx;
	line-height: 70rpx;
	font-size: 32rpx;
	text-align: center;
}
.dailogView .formItem{
	display: inline-block;
	width: 100%;
	border-bottom: 1rpx solid #eeeeee;
	height: 70rpx;
	line-height: 70rpx;
	font-size: 32rpx;
}
.dailogView .formItem:last-child{
	border: 0;
}
.dailogView .formItem input{
	display: inline-block;
	vertical-align: middle;
	height: 50rpx;
	line-height: 50rpx;
	width: 100%;
}

